#include <iostream>
using namespace std;
int main(){
	string s="hello";
	cout<<s<<endl;
	cout<<s[0]<<endl;
	cout<<s[0]+2<<endl;
	cout<<char(s[0]+2)<<endl;



	return 0;
}
